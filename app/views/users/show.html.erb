                    </div>
                </div><!-- .col-md-12 close -->
            </div><!-- .row close -->
        </div><!-- .container close -->
	</nav><!-- header close -->
    <!--
    Slider start
    ============================== -->
  
    <!--
    about-us start
    ============================== -->
    <section id="about-us">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="block">
                        <h1 class="heading wow fadeInUp" data-wow-duration="400ms" data-wow-delay="500ms" ><%= @user.name %>'s Profile</span> </br> 
							<p style="margin: 0; padding: 0;">Email: <%= @user.email if @user.email %></p>
							<p style="margin: 0; padding: 0;" id="notice"><%= notice %></p>
							<p style="margin: 0: padding: 0;">
							  <%= image_tag @user.avatar.url(:large) %>
							</p>

							<%= link_to 'Edit', edit_user_path(@user) %> |
							<%= link_to 'Back', users_path %>
                        </h1>
                        <p class="wow fadeInUp" data-wow-duration="300ms" data-wow-delay="600ms"></p>
                    </div>
                </div><!-- .col-md-12 close -->
            </div><!-- .row close -->
        </div><!-- .containe close -->
    </section><!-- #call-to-action close -->
